package IEexp;

import org.openqa.selenium.WebDriver;
import org.openqa.selenium.firefox.FirefoxDriver;
import org.openqa.selenium.ie.InternetExplorerDriver;

public class Driver {

	public static void main(String[] args) {
		System.setProperty("webdriver.ie.driver", "C:\\Users\\vvini\\Desktop\\selenium\\IEDriverServer.exe");
		WebDriver vini=new InternetExplorerDriver();
		vini.get("www.amazon.com");
	
		WebDriver vinitha=new FirefoxDriver();
		vini.get("https://www.facebook.com/");
		vini.close();
		
		

	}

}
